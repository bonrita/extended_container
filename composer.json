{
  "name": "drupal/extended_container",
  "description": "Extend the capabilities of the container.",
  "type": "drupal-module",
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "keywords": ["container"],
  "authors": [
    {
      "name": "Bonrita",
      "homepage": "https://www.drupal.org/u/bonrita",
      "role": "Maintainer"
    }
  ],
  "require": {
    "symfony/config": "^3.4"
  },
  "extra": {
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "patches": {
      "drupal/core": {
        "Combine issues 2961380, 3108020, 3108039": "https://www.drupal.org/files/issues/2020-01-24/combined-patches-for-issues-2961380-3108020-3108039.patch",
        "Support custom tags": "https://www.drupal.org/files/issues/2020-01-24/3108309-4-complete.patch",
        "Update the Yaml file Loader functionality": "https://www.drupal.org/files/issues/2020-01-28/3108565-6-complete.patch"
      }
    }
  }
}
