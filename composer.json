{
  "name": "drupal/extended_container",
  "description": "Extend the capabilities of the container.",
  "type": "drupal-module",
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "keywords": ["container"],
  "authors": [
    {
      "name": "Bonrita",
      "homepage": "https://www.drupal.org/u/bonrita",
      "role": "Maintainer"
    }
  ],
  "extra": {
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "patches": {
      "drupal/core": {
        "Support ServiceClosureArgument in OptimizedPhpArrayDumper": "https://github.com/bonrita/drupal_core_patches/blob/master/drupal8/drupal-8-dependency-injection-service-closure-argument.patch"
      }
    }
  }
}
